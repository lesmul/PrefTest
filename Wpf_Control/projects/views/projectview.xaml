      <DataTemplate x:Key="TextHeaderTemplate">
        <TextBlock TextTrimming="CharacterEllipsis" Text="{Binding}" />
      </DataTemplate>
      <DataTemplate x:Key="templateDocumentsDocumentType">
        <TextBlock HorizontalAlignment="Left" VerticalAlignment="Center" Text="{Binding Path=DocumentTypeName}" />
      </DataTemplate>
      <DataTemplate x:Key="templateDocumentsDocumentDescription">
        <TextBlock HorizontalAlignment="Left" VerticalAlignment="Center" Text="{Binding Path=Description}" />
      </DataTemplate>
      <DataTemplate x:Key="templateDocumentsDocumentName">
        <StackPanel Orientation="Horizontal" TextBlock.FontSize="12">
          <ToggleButton Name="Expander" ClickMode="Press" Style="{StaticResource ExpandCollapseToggleStyle}" Margin="{Binding Level, Converter={StaticResource LevelToIndentConverter}, RelativeSource={RelativeSource AncestorType={x:Type src:TreeListViewItem}}}" IsChecked="{Binding Path=IsExpanded}" />
          <Image Width="32" Height="32" Source="{Binding Path=IconSource}" />
          <TextBlock HorizontalAlignment="Left" VerticalAlignment="Center" Text="{Binding Path=DocumentName}" />
        </StackPanel>
        <DataTemplate.Triggers>
          <DataTrigger Value="False" Binding="{Binding Path=HasItems, RelativeSource={RelativeSource AncestorType={x:Type src:TreeListViewItem}}}">
            <Setter TargetName="Expander" Property="UIElement.Visibility" Value="Hidden" />
          </DataTrigger>
        </DataTemplate.Triggers>
      </DataTemplate>
      <DataTemplate x:Key="RemnantIncrementTemplate">
        <StackPanel HorizontalAlignment="Right" Orientation="Horizontal" TextBlock.FontSize="12">
          <TextBox Name="RemnantIncrementTextBox" TextAlignment="Right" HorizontalAlignment="Right" Text="{Binding Path=RemnantIncrementAsPercentage, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource resNumberConverter}}" IsReadOnly="{Binding Path=IsCostIncrementEditable, Converter={StaticResource boolNegatorConverter}}" GotKeyboardFocus="OnTemplateItemGotFocus" />
          <TextBlock Name="RemnantIncrementTextBlock" Text="%" TextAlignment="Right" HorizontalAlignment="Right" VerticalAlignment="Center" />
          <Button Name="RemnantIncrementButton" Width="18" Height="18" HorizontalAlignment="Right" VerticalAlignment="Center" Click="RemnantIncrementButton_Click">
            <Image Margin="-2,-2,-2,-2" Width="Auto" Height="Auto" Source="{StaticResource IconUndo}" />
          </Button>
        </StackPanel>
        <DataTemplate.Triggers>
          <DataTrigger Value="true" Binding="{Binding Path=IsLabourCost, UpdateSourceTrigger=PropertyChanged}">
            <Setter TargetName="RemnantIncrementTextBox" Property="UIElement.Visibility" Value="Hidden" />
            <Setter TargetName="RemnantIncrementTextBlock" Property="UIElement.Visibility" Value="Hidden" />
            <Setter TargetName="RemnantIncrementButton" Property="UIElement.Visibility" Value="Hidden" />
          </DataTrigger>
          <DataTrigger Value="true" Binding="{Binding Path=IsWithoutGroup, UpdateSourceTrigger=PropertyChanged}">
            <Setter TargetName="RemnantIncrementTextBox" Property="UIElement.Visibility" Value="Hidden" />
            <Setter TargetName="RemnantIncrementTextBlock" Property="UIElement.Visibility" Value="Hidden" />
            <Setter TargetName="RemnantIncrementButton" Property="UIElement.Visibility" Value="Hidden" />
          </DataTrigger>
          <DataTrigger Value="true" Binding="{Binding Path=IsRemnantIncrementOverriden, UpdateSourceTrigger=PropertyChanged}">
            <Setter TargetName="RemnantIncrementTextBox" Property="Control.Background" Value="#FFF0FFF0" />
            <Setter TargetName="RemnantIncrementButton" Property="UIElement.Visibility" Value="Visible" />
          </DataTrigger>
          <DataTrigger Value="false" Binding="{Binding Path=IsRemnantIncrementOverriden, UpdateSourceTrigger=PropertyChanged}">
            <Setter TargetName="RemnantIncrementTextBox" Property="Control.Background" Value="#FFFFFFFF" />
            <Setter TargetName="RemnantIncrementButton" Property="UIElement.Visibility" Value="Hidden" />
          </DataTrigger>
          <DataTrigger Value="false" Binding="{Binding Path=IsCostIncrementEditable, UpdateSourceTrigger=PropertyChanged}">
            <Setter TargetName="RemnantIncrementTextBox" Property="Control.Background" Value="#FFA9A9A9" />
            <Setter TargetName="RemnantIncrementButton" Property="UIElement.Visibility" Value="Hidden" />
          </DataTrigger>
        </DataTemplate.Triggers>
      </DataTemplate>
